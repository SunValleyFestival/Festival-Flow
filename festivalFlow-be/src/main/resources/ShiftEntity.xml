<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.sunvalley.festivalFlowbe.entity.ShiftEntity">
        <table name="shift" schema="FestivalFlow" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="description">
                <column name="description"/>
            </basic>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="locationId">
                <column name="location_id"/>
            </basic>
            <basic name="time">
                <column name="time"/>
            </basic>
            <basic name="day">
                <column name="day"/>
            </basic>
            <basic name="maxCollaborator">
                <column name="maxCollaborator"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
