<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.sunvalley.festivalFlowbe.entity.AssociationEntity">
        <table name="association" schema="FestivalFlow" catalog=""/>
        <id-class class="com.sunvalley.festivalFlowbe.entity.AssociationEntityPK"/>
        <attributes>
            <id name="shiftId">
                <column name="shift_id"/>
            </id>
            <id name="collaboratorId">
                <column name="collaborator_id"/>
            </id>
            <basic name="status">
                <column name="status"/>
            </basic>
            <basic name="request">
                <column name="request"/>
            </basic>
            <basic name="collaboratorFriendId">
                <column name="collaborator_friend_id"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
